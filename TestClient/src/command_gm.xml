<?xml version="1.0" encoding="UTF-8"?><httpcommands>	<module name="gmTool">		<command id="0x0D0001" name="itemList" desc=" 物品信息列表">			<request>				<string>gm_name</string>				<string>gm_pwd</string>			</request>			<response>				<list name="list_item"><!-- list包含一个int型列表长度 -->					<string>id</string>					<string>index</string>					<string>name</string>					<string>nick_name</string>					<string>tree_new_bee</string>					<string>package_icon</string>					<int>unlock_stage</int>					<int>currency</int>					<int>price</int>					<int>tab</int>					<int>order</int>					<int>label</int>					<int>drop_rate</int>					<int>score_consumption</int>					<int>start_item</int>				</list>			</response>		</command>		<command id="0x0D0002" name="change_item_tab" desc=" 修改物品tab类型">			<request>				<string>gm_name</string>				<string>gm_pwd</string>				<string>id</string>				<int>tab</int>			</request>			<response>				<string>result</string>			</response>		</command>		<command id="0x0D0003" name="change_item_order" desc="修改物品的顺序">			<request>				<string>gm_name</string>				<string>gm_pwd</string>				<string>id</string>				<int>order</int>			</request>			<response>				<string>result</string>			</response>		</command>		<command id="0x0D0004" name="change_item_top" desc="上架">			<request>				<string>gm_name</string>				<string>gm_pwd</string>				<string>id</string>			</request>			<response>				<string>result</string>			</response>		</command>		<command id="0x0D0005" name="change_item_down" desc=" 下架">			<request>				<string>gm_name</string>				<string>gm_pwd</string>				<string>id</string>			</request>			<response>				<string>result</string>			</response>		</command>		<command id="0x0D0006" name="change_item_price" desc=" 修改物品价格">			<request>				<string>gm_name</string>				<string>gm_pwd</string>				<string>id</string>				<int>price</int>			</request>			<response>				<string>result</string>			</response>		</command>		<command id="0x0D0007" name="change_item_label" desc=" 修改物品标签">			<request>				<string>gm_name</string>				<string>gm_pwd</string>				<string>id</string>				<int>label</int>			</request>			<response>				<string>result</string>			</response>		</command>		<command id="0x0D0008" name="change_item_nick_name" desc=" 修改物品名字">			<request>				<string>gm_name</string>				<string>gm_pwd</string>				<string>id</string>				<string>nick_name</string>			</request>			<response>				<string>result</string>			</response>		</command>		<command id="0x0D0009" name="change_item_currency" desc=" 修改物品币种">			<request>				<string>gm_name</string>				<string>gm_pwd</string>				<string>id</string>				<int>currency</int>			</request>			<response>				<string>result</string>			</response>		</command>		<command id="0x0D000A" name="change_item_icon" desc=" 修改合物品图标">			<request>				<string>gm_name</string>				<string>gm_pwd</string>				<string>id</string>				<string>icon</string>			</request>			<response>				<string>result</string>			</response>		</command>		<command id="0x0D000B" name="gather_sell_list" desc=" 合售包物品列表">			<request>				<string>gm_name</string>				<string>gm_pwd</string>			</request>			<response>				<list name="list_gather_sell"><!-- list包含一个int型列表长度 -->					<string>id</string>					<string>nick_name</string>					<string>desc</string>					<string>package_icon</string>					<int>unlock_stage</int>					<int>currency</int>					<int>price</int>					<int>tab</int>					<int>order</int>					<int>label</int>					<list name="list_item">						<string>item_index</string>						<int>num</int>					</list>				</list>			</response>		</command>		<command id="0x0D000C" name="change_gather_sell_item_num"			desc=" 修改合售包物品,包括添加及修改数量">			<request>				<string>gm_name</string>				<string>gm_pwd</string>				<string>id</string>				<list name="item_type">					<string>item_index</string>					<int>num</int>				</list>			</request>			<response>				<string>result</string>			</response>		</command>		<command id="0x0D000D" name="remove_gather_sell_item" desc="删除合售包内指定物品">			<request>				<string>gm_name</string>				<string>gm_pwd</string>				<string>id</string>				<string>item_index</string>			</request>			<response>				<string>result</string>			</response>		</command>		<command id="0x0D000E" name="item_info" desc="获取指定物品的详细信息">			<request>				<string>gm_name</string>				<string>gm_pwd</string>				<string>id</string>			</request>			<response>				<string>id</string>				<string>index</string>				<string>name</string>				<string>nick_name</string>				<string>tree_new_bee</string>				<string>package_icon</string>				<int>unlock_stage</int>				<int>currency</int>				<int>price</int>				<int>tab</int>				<int>order</int>				<int>label</int>				<int>drop_rate</int>				<int>score_consumption</int>				<int>start_item</int>			</response>		</command>		<command id="0x0d000F" name="gatherSellInfo">			<request>				<string>gm_name</string>				<string>gm_pwd</string>				<string>id</string>			</request>			<response>				<string>id</string>				<string>nick_name</string>				<string>desc</string>				<string>package_icon</string>				<int>unlock_stage</int>				<int>currency</int>				<int>price</int>				<int>tab</int>				<int>order</int>				<int>label</int>				<list name="list_item">					<string>item_index</string>					<int>num</int>				</list>			</response>		</command>		<!-- 活动 -->		<command id="0x0D0101" name="list_activity" desc=" 活动信息列表">			<request>				<string>gm_name</string>				<string>gm_pwd</string>			</request>			<response>				<list name="list">					<string>id</string>					<string>index</string>					<string>name</string>					<string>touch_type</string>					<int>touch_interval</int>					<int>availability</int>					<string>times</string>					<int>period</int>					<string>reward_per_grade</string>					<string>material_reward</string>					<int>exchange_rate</int>					<int>material_available</int>					<int>comfort_coin</int>					<string>times_1</string>					<int>period_1</int>					<string>reward_per_grade_1</string>					<string>material_reward_1</string>					<int>exchange_rate_1</int>					<int>material_available_1</int>					<int>comfort_coin_1</int>					<string>times_2</string>					<int>period_2</int>					<string>reward_per_grade_2</string>					<string>material_reward_2</string>					<int>exchange_rate_2</int>					<int>material_available_2</int>					<int>comfort_coin_2</int>				</list>			</response>		</command>		<command id="0x0d0102" name="setOpenOrClose" desc=" 设置活动手动开关">			<request>				<string>gm_name</string>				<string>gm_pwd</string>				<string>id</string>				<int>isOpen</int><!-- 0关 1开 -->			</request>			<response>				<string>result</string>			</response>		</command>		<command id="0x0d0103" name="setActivityTime" desc="设置活动时间">			<request>				<string>gm_name</string>				<string>gm_pwd</string>				<string>id</string>				<int>num</int><!-- 值为1,2,3对应times,times_1,times_2以下此值意义相同 -->				<string>time</string><!-- 2013-4-25 13:00:00#2093-4-25 15:00:00 -->				<int>index</int><!-- 值为1,2,3对应使用数据的序号 -->			</request>			<response>				<string>result</string>			</response>		</command>		<command id="0x0d0104" name="SET_PER_TIMES_AWARD_INTERVAL"			desc=" 设定活动币的有效周期，也可直接设定为按自然月更换（period）">			<request>				<string>gm_name</string>				<string>gm_pwd</string>				<string>id</string>				<int>num</int><!-- 值为1,2,3 -->				<int>minute</int>			</request>			<response>				<string>result</string>			</response>		</command>		<command id="0x0d0105" name="SET_REWARD_PER_GRADE" desc=" 设定连续多少次领奖以后奖品会提升（最多提升三次）">			<request>				<string>gm_name</string>				<string>gm_pwd</string>				<string>id</string>				<int>num</int><!-- 值为1,2,3 -->				<string>grade</string><!-- 标准grade格式:10,20,30 -->			</request>			<response>				<string>result</string>			</response>		</command>		<command id="0x0d0106" name="SET_MATERIAL_REWARD"			desc="设定3个奖励级别各自的奖励兑换币数量material_reward">			<request>				<string>gm_name</string>				<string>gm_pwd</string>				<string>id</string>				<int>num</int><!-- 值为1,2,3 -->				<string>material</string><!-- 标准material格式:2,3,5 -->			</request>			<response>				<string>result</string>			</response>		</command>		<command id="0x0d0107" name="SET_MATERIAL_AVAILABLE"			desc=" 可以设置目前奖励的兑换币和魔幻烤箱不同步，并且手动设置目前指定的兑换币 material_available">			<request>				<string>gm_name</string>				<string>gm_pwd</string>				<string>id</string>				<int>num</int><!-- 值为1,2,3 -->				<int>count</int><!--值为1....12 -->			</request>			<response>				<string>result</string>			</response>		</command>		<command id="0x0d0108" name="SET_MESSAGE_INFO" desc="设定短信内容">			<request>				<string>gm_name</string>				<string>gm_pwd</string>				<string>info</string>			</request>			<response>				<string>result</string>			</response>		</command>		<command id="0x0d0109" name="SET_FRIEND_POINT_REWARD_ITEM"			desc="友情点数奖励的四个道具设定（没有的时候随机指定用动物饼干购买的道具）">			<request>				<string>gm_name</string>				<string>gm_pwd</string>				<list name="item_list">					<int>key</int><!-- 值为0,1,2,3 -->					<string>itemID</string>				</list>			</request>			<response>				<string>result</string>			</response>		</command>		<command id="0x0d010A" name="SET_FRIEND_POINT_USE_VALUE" desc=" 可手动调整兑换所需友情点数">			<request>				<string>gm_name</string>				<string>gm_pwd</string>				<list name="point_list">					<int>key</int><!-- 值为0,1,2,3 -->					<string>point</string>				</list>			</request>			<response>				<string>result</string>			</response>		</command>		<command id="0x0d010B" name="SET_EXCHANGE_RATE"			desc="友情点数对动物饼干的兑换比例（关系到兑换价格的自动计算，exchange_rate）">			<request>				<string>gm_name</string>				<string>gm_pwd</string>				<string>id</string>				<int>num</int><!-- 值为1,2,3 -->				<int>rate</int><!--值为1....100 -->			</request>			<response>				<string>result</string>			</response>		</command>		<command id="0x0d010C" name="SET_INVITATION_INFO" desc=" 编辑发送给邀请者和受邀者的系统消息文本内容">			<request>				<string>gm_name</string>				<string>gm_pwd</string>				<string>invitation</string><!-- 邀请者 -->				<string>be_invitation</string><!-- 被邀请者 -->			</request>			<response>				<string>result</string>			</response>		</command>		<command id="0x0d010D" name="SET_OVEN_REWARD_ITEM" desc=" 设定烤箱可兑换的四个道具">			<request>				<string>gm_name</string>				<string>gm_pwd</string>				<list name="itemList"><!-- list长度必须为4 -->					<string>itemID</string>				</list>			</request>			<response>				<string>result</string>			</response>		</command>		<command id="0x0d010E" name="SET_COMFORT_COIN" desc=" 设定未中奖时的安慰奖银币数量（comfort_coin）">			<request>				<string>gm_name</string>				<string>gm_pwd</string>				<string>id</string>				<int>num</int><!-- 值为1,2,3 -->				<int>coin</int>			</request>			<response>				<string>result</string>			</response>		</command>		<command id="0x0D0110" name="REQUEST_QUERY_ACTIVITY" desc=" 活动详细信息">			<request>				<string>gm_name</string>				<string>gm_pwd</string>				<string>id</string>			</request>			<response>				<string>id</string>				<string>index</string>				<string>name</string>				<string>touch_type</string>				<int>touch_interval</int>				<int>availability</int>				<string>times</string>				<int>period</int>				<string>reward_per_grade</string>				<string>material_reward</string>				<int>exchange_rate</int>				<int>material_available</int>				<int>comfort_coin</int>				<string>times_1</string>				<int>period_1</int>				<string>reward_per_grade_1</string>				<string>material_reward_1</string>				<int>exchange_rate_1</int>				<int>material_available_1</int>				<int>comfort_coin_1</int>				<string>times_2</string>				<int>period_2</int>				<string>reward_per_grade_2</string>				<string>material_reward_2</string>				<int>exchange_rate_2</int>				<int>material_available_2</int>				<int>comfort_coin_2</int>			</response>		</command>		<command id="0x0d0111" name="SET_FRIEND_POINT_INTERVAL" desc=" 设置友情点数间隔">			<request>				<string>gm_name</string>				<string>gm_pwd</string>				<int>num</int><!-- 单位:分钟 -->			</request>			<response>				<string>result</string>			</response>		</command>		<command id="0x0d0201" name="SET_MAXIMUM_HEART_REFRESH"			desc="设定和修改缺省最大心数（maximum_heart_refresh）">			<request>				<string>gm_name</string>				<string>gm_pwd</string>				<int>num</int>			</request>			<response>				<string>result</string>			</response>		</command>		<command id="0x0d0202" name="SET_PERIOD_HEART_REFRESH"			desc=" 设定和修改心数刷新时间周期（秒，period_heart_refresh）">			<request>				<string>gm_name</string>				<string>gm_pwd</string>				<int>num</int>			</request>			<response>				<string>result</string>			</response>		</command>		<command id="0x0d0203" name="SET_HEART_PRICE_COIN" desc=" 设定和修改买心的银币价格（heart_price_coin）">			<request>				<string>gm_name</string>				<string>gm_pwd</string>				<int>num</int>			</request>			<response>				<string>result</string>			</response>		</command>		<command id="0x0d0204" name="SET_GINGER_UNLOCK_STAGE"			desc="设定和修改解锁姜饼人的三个关卡（三个数字输入，ginger_unlock_stage）">			<request>				<string>gm_name</string>				<string>gm_pwd</string>				<string>stage</string><!-- 格式应为:12,24,45 -->			</request>			<response>				<string>result</string>			</response>		</command>		<command id="0x0d0301" name="SET_LINES_PER_LEVEL" desc=" 修改各个单独级别的行数（lines_per_level）">			<request>				<string>gm_name</string>				<string>gm_pwd</string>				<int>num</int>			</request>			<response>				<string>result</string>			</response>		</command>		<command id="0x0d0302" name="SET_SCORE_CONSUMPTION"			desc=" 设定各个道具是否有效可以使用（score_consumption）">			<request>				<string>gm_name</string>				<string>gm_pwd</string>				<string>id</string>				<int>num</int>			</request>			<response>				<string>result</string>			</response>		</command>		<command id="0x0d0303" name="SET_DROP_RATE" desc=" 设定各个道具是否可以在关卡中掉落drop_rate">			<request>				<string>gm_name</string>				<string>gm_pwd</string>				<string>id</string>				<int>num</int>			</request>			<response>				<string>result</string>			</response>		</command>		<command id="0x0d0401" name="LIST_GINGER" desc="姜饼人列表">			<request>				<string>gm_name</string>				<string>gm_pwd</string>			</request>			<response>				<list name="list">					<string>id</string>					<string>index</string>					<string>name</string>					<string>des</string>					<string>skill</string>					<int>availability</int>					<string>animation</string>					<int>activate_rate</int>					<string>item_1</string>					<int>item_num_1</int>					<string>item_2</string>					<int>item_num_2</int>					<string>item_3</string>					<int>item_num_3</int>					<string>item_4</string>					<int>item_num_4</int>					<string>item_5</string>					<int>item_num_5</int>					<string>item_6</string>					<int>item_num_6</int>				</list>			</response>		</command>		<command id="0x0d0402" name="SET_ACTIVATE_RATE"			desc="修改定各个姜饼人在各个技能级别下的触发概率（四个姜饼人，每个5个级别activate_rate）">			<request>				<string>gm_name</string>				<string>gm_pwd</string>				<string>id</string>				<int>num</int>			</request>			<response>				<string>result</string>			</response>		</command>		<command id="0x0d0403" name="SET_UNLOCK_PRICE"			desc=" 修改付费解锁所需的三次动物饼干价格（unlock_price_1,……）">			<request>				<string>gm_name</string>				<string>gm_pwd</string>				<int>num</int><!-- 值为:1 or 2 or3 -->				<int>price</int>			</request>			<response>				<string>result</string>			</response>		</command>		<command id="0x0d0404" name="SET_AVAILABILITY" desc=" 设定各个姜饼人是否开放（availability）">			<request>				<string>gm_name</string>				<string>gm_pwd</string>				<string>id</string>				<int>num</int><!-- 0:关闭 1:开放 -->			</request>			<response>				<string>result</string>			</response>		</command>		<command id="0x0d0405" name="SET_GINGER_ITEMS"			desc="设定各个技能级别升级所需的最多六种道具的种类和数量（item_1,item_num_1,……）">			<request>				<string>gm_name</string>				<string>gm_pwd</string>				<string>id</string>				<int>index</int><!-- 值为:1...6 -->				<string>item_index</string>				<int>item_num</int>			</request>			<response>				<string>result</string>			</response>		</command>		<command id="0x0d0406" name="REQUEST_QUERY_GINGER_INFO" desc="姜饼人详细信息">			<request>				<string>gm_name</string>				<string>gm_pwd</string>				<string>id</string>			</request>			<response>				<string>id</string>				<string>index</string>				<string>name</string>				<string>des</string>				<string>skill</string>				<int>availability</int>				<string>animation</string>				<int>activate_rate</int>				<string>item_1</string>				<int>item_num_1</int>				<string>item_2</string>				<int>item_num_2</int>				<string>item_3</string>				<int>item_num_3</int>				<string>item_4</string>				<int>item_num_4</int>				<string>item_5</string>				<int>item_num_5</int>				<string>item_6</string>				<int>item_num_6</int>			</response>		</command>		<command id="0x0d0501" name="LIST_BOUNCEBALL" desc="反弹球信息列表">			<request>				<string>gm_name</string>				<string>gm_pwd</string>			</request>			<response>				<list name="list_bounceball">					<string>id</string>					<string>index</string>					<string>name</string>					<string>description</string>					<string>icon</string>					<string>plate</string>					<string>previous</string>					<int>previous_purchase</int>					<int>availability</int>					<int>price</int>					<int>parameter_1</int>				</list>			</response>		</command>		<command id="0x0d0502" name="SET_BOUNCEBALL_AVAILABILITY"			desc="设定各个反弹球的可用性（availability）">			<request>				<string>gm_name</string>				<string>gm_pwd</string>				<string>id</string>				<int>num</int><!-- 0:关闭 1:开放 -->			</request>			<response>				<string>result</string>			</response>		</command>		<command id="0x0d0503" name="SET_BOUNCEBALL_PRICE" desc=" 设定各个反弹球的购买价格（price）">			<request>				<string>gm_name</string>				<string>gm_pwd</string>				<string>id</string>				<int>num</int>			</request>			<response>				<string>result</string>			</response>		</command>		<command id="0x0d0504" name="SET_BOUNCEBALL_PREVIOUS_PURCHASE"			desc="设定各个反弹球升级解锁需购买的数量（previous_purchase">			<request>				<string>gm_name</string>				<string>gm_pwd</string>				<string>id</string>				<int>num</int>			</request>			<response>				<string>result</string>			</response>		</command>		<command id="0x0d0505" name="SET_BOUNCEBALL_RATE" desc=" 设定反弹球的基准反弹率（bounce_rate">			<request>				<string>gm_name</string>				<string>gm_pwd</string>				<int>num</int>			</request>			<response>				<string>result</string>			</response>		</command>		<command id="0x0d0506" name="REQUEST_QUERY_BOUNCEBALL_INFO"			desc="反弹球详细信息">			<request>				<string>gm_name</string>				<string>gm_pwd</string>				<string>id</string>			</request>			<response>				<string>id</string>				<string>index</string>				<string>name</string>				<string>description</string>				<string>icon</string>				<string>plate</string>				<string>previous</string>				<int>previous_purchase</int>				<int>availability</int>				<int>price</int>				<int>parameter_1</int>			</response>		</command>		<command id="0x0d0601" name="SET_BUBBLE_COLUMN" desc="设定泡泡发射所占体积比例（bubble_column）">			<request>				<string>gm_name</string>				<string>gm_pwd</string>				<int>num</int>			</request>			<response>				<string>result</string>			</response>		</command>		<command id="0x0d0602" name="SET_MAXIMUM_BUDDY" desc=" 设定用户好友列表最大长度（maximum_buddy）">			<request>				<string>gm_name</string>				<string>gm_pwd</string>				<int>num</int>			</request>			<response>				<string>result</string>			</response>		</command>		<command id="0x0d0603" name="SET_MAXIMUM_WISHLIST" desc=" 设定心愿单最大