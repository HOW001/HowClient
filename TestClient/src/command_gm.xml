</string>				<int>queryType</int><!-- 1:赠送者ID查询 2:赠送者name查询 3:接收者ID查询 4:接收者Name 其他值可以作为后期扩展使用 -->				<string>value</string><!-- id or name -->			</request>			<response>				<list name="log_List">					<string>id</string>					<string>Giving_id</string>					<string>Giving_name</string>					<string>Be_giving_id</string>					<string>Be_giving_name</string>					<string>Giving_type</string>					<string>Index</string>					<int>Num</int>					<string>Create_time</string>				</list>			</response>		</command>		<command id="0x0d0704" name="REQUEST_QUERY_MAIL_LOG" desc=" 查询用户邮件日志">			<request>				<string>gm_name</string>				<string>gm_pwd</string>				<int>queryType</int><!-- 1.发送者ID查询 2.发送者name查询 3.接收者ID查询 4.接收者name查询 -->				<string>value</string><!-- id or name -->			</request>			<response>				<list name="log_List">					<string>id</string>					<string>Receiver</string>					<string>receiverName</string>					<string>Sender</string>					<string>senderName</string>					<string>Send_time</string>					<string>Content</string>					<int>Type</int>					<string>Values</string>				</list>			</response>		</command>		<command id="0x0d0705" name="REQUEST_GIVING_PLAYER" desc=" 赠送玩家道具\动物饼干\银币">			<request>				<string>gm_name</string>				<string>gm_pwd</string>				<int>type</int><!-- 1.为赠送道具 2.赠送动物饼干 3.银币 -->				<string>index</string><!-- 道具index or Gold or Sliver -->				<int>count</int><!-- 赠送的数量 -->				<list name="playerList"><!-- 此列表长度为0时表示所有玩家 -->					<string>player_name</string>				</list>			</request>			<response>				<string>result</string>			</response>		</command>		<command id="0x0d0706" name="REQUEST_SET_PLAYER_STATUS" desc=" 封停解封账号">			<request>				<string>gm_name</string>				<string>gm_pwd</string>				<int>queryType</int><!-- 1.为玩家id查询 2.为玩家name查询 -->				<string>value</string><!-- id or name -->				<int>status</int><!-- 0:解封帐户 1:暂时封停角色 2.永久封停角色 -->			</request>			<response>				<string>result</string>			</response>		</command>		<command id="0x0d0707" name="REQUEST_SEND_MAIL" desc=" 发送邮件">			<request>				<string>gm_name</string>				<string>gm_pwd</string>				<string>content</string>				<string>sendTime</string><!-- 格式为:2013-05-31 13:33:33 -->				<int>time_len</int><!-- 邮件有效期,单位分钟 -->				<list name="palyer_list"><!-- 此列表为0时表示发送给所有玩家 -->					<string>player_name</string>				</list>			</request>			<response>				<string>result</string>			</response>		</command>		<command id="0x0d0709" name="REQUEST_CONFIG_LIST" desc="配置信息列表">			<request>				<string>gm_name</string>				<string>gm_pwd</string>			</request>			<response>				<int>bounce_rate</int><!-- 反弹系数 -->				<int>heart_price_coin</int><!-- 心的银币价格 -->				<int>period_heart_refresh</int><!-- 心的刷新周期 单位：秒 -->				<int>maximum_heart_refresh</int><!-- 心的最大数量 -->				<int>billboard_length</int><!-- 冒险、街机和善人三个榜的显示长度 -->				<int>maximum_buddy</int><!-- 好友列表的最大长度 -->				<int>maximum_wishlist</int><!-- 心愿单能容纳道具种类的数量 -->				<int>maximum_message</int><!-- 消息存储的条数上限 -->				<int>cache_message</int><!-- 消息在服务器缓存的条数上限 -->				<int>message_length</int><!-- 消息的长度（汉字数量） -->				<int>maximum_item_stack</int><!-- 同种道具最大拥有数量 -->				<string>ginger_unlock_stage</string><!-- 姜饼人解锁的三个关卡 -->				<int>unlock_price_1</int><!-- 提前解锁第二个姜饼人的价格 -->				<int>unlock_price_2</int><!-- 提前解锁第三个姜饼人的价格 -->				<int>unlock_price_3</int><!-- 提前解锁第四个姜饼人的价格 -->				<int>bubble_column</int><!-- 泡泡在飞行时的大小判定比例 -->				<int>lines_per_level</int><!-- 街机模式下每一个级别的行数 -->				<list name="oven_award_item"><!-- oven_award_item -->					<string>item_index</string>				</list>				<list name="friend_point_award_item"><!-- 友情点数兑换物品列表 key:0,1,2,3 value:物品ID -->					<string>key</string>					<string>value</string>				</list>				<list name="friend_point_use_value"><!-- 友情点数兑换所使用的点数列表 key:0,1,2,3 value:1,3,5,10 -->					<string>key</string>					<int>value</int>				</list>				<int>friend_point_interval</int><!-- 友情点数物品刷新间隔,单位分钟 -->				<string>last_refresh_friend_point</string><!-- 上一次刷新时间 -->				<string>message_info</string><!-- 短信内容 -->				<string>invitationer_system_info</string><!-- 邀请者系统信息 -->				<string>be_invitationer_system_info</string><!-- 被邀请者系统信息 -->			</response>		</command>		<command id="0x0d070A" name="REQUEST_PLAYER_INFO" desc="请求玩家信息">			<request>				<string>gm_name</string>				<string>gm_pwd</string>				<int>queryType</int><!-- 1.为玩家id查询 2.为玩家name查询 -->				<string>value</string><!-- id or name -->			</request>			<response>				<boolean>isFind</boolean><!-- 是否存在该玩家,如果为false,则以下数据不进行读取 -->				<string>Id</string>				<string>Name</string>				<string>Pwd</string>				<string>Sex</string>				<string>Register_time</string>				<string>Last_login_time</string>				<string>Current_use_pet</string>				<int>Gold_coin</int>				<int>Silver_coin</int>				<int>Player_status</int>			</response>		</command>		<command id="0x0d070B" name="REQUEST_QUERY_PLAYER" desc="查询玩家是否存在">			<request>				<string>gm_name</string>				<string>gm_pwd</string>				<int>queryType</int><!-- 1.为玩家id查询 2.为玩家name查询 -->				<string>value</string><!-- id or name -->			</request>			<response>				<boolean>isFind</boolean><!-- 是否存在该玩家,如果为false,则以下数据不进行读取 -->			</response>		</command>		<command id="0x0d070C" name="REQUEST_PLAYER_INFO_FOR_NAME"			desc="对玩家名字进行模糊查询">			<request>				<string>gm_name</string>				<string>gm_pwd</string>				<string>value</string><!-- name -->				<int>page</int><!-- 查第几页 -->				<int>count</int><!-- 每页多少 -->			</request>			<response>				<list name="player_list">					<string>Id</string>					<string>Name</string>					<string>Pwd</string>					<string>Sex</string>					<string>Register_time</string>					<string>Last_login_time</string>					<string>Current_use_pet</string>					<int>Gold_coin</int>					<int>Silver_coin</int>					<int>Player_status</int>				</list>			</response>		</command>	</module></httpcommands>